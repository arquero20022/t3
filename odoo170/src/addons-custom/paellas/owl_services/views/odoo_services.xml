    <field name="model">custom.barcode.model</field>
    <field name="arch" type="xml">
        <form>
            <sheet>
                <group>
                    <field name="name"/>
                    <field name="barcode_value"/>
                </group>
            </sheet>
        </form>
    </field>
</record>
